<ul class= 'research'>	
	<% findings = point.findings.sort {|a,b| b.research.score <=> a.research.score}%>
	<% findings.each do |finding| %>
		<%= render 'questions/evidence/findings', finding: finding, point: point %>
	<%end%> 
</ul>	
<%= link_to "View less research("+point.findings.count('research_id', distinct:true).to_s+")", less_research_question_path(point: point), class: "pad-left",remote: true%>